{
  "name": "wa-corp-scraper",
  "version": "1.0.0",
  "description": "",
  "main": "dist/cli.js",
  "scripts": {
    "forever-stop": "forever stop",
    "start": "nodemon --watch '**/*.ts' --exec ts-node index.ts",
    "start-app": "tsc && yarn run serve",
    "serve": "node dist/index.js",
    "corps": "tsc && dist/cli.js",
    "stopall": "forever stopall",
    "clean": "rm -rf ./csv-output-*/* && rm -rf ./csv-output*/* && rm -rf ./logs/* && rm -rf ./dist/*",
    "count": "cat ./csv-output/* | wc -l",
    "save": "git add . && git commit -m ':octocat: :four_leaf_clover: please work' && git push origin develop",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "specialized": "yarn run corps company_keyword_scraper --c=1 --id=-1 --keyword=specialized",
    "illustrations": "yarn run corps company_keyword_scraper --c=1 --id=-1 --keyword=illustrations",
    "ecocarcafe": "yarn run corps company_keyword_scraper --c=1 --id=-1 --keyword=ecocarcafe",
    "lawfirm": "yarn run corps company_keyword_scraper --c=1 --id=-1 --keyword=lawfirm",
    "all_business": "yarn run wllc && yarn run wllcp && yarn run wllp && yarn run wlp && yarn run wpllc && yarn run wpllp && yarn run wpsc && yarn run wpc && yarn run wnc && yarn run wpbc",
    "test": "sudo yarn -g && corps",
    "wllc": "yarn run corps company_info_scraper --concurency=1 --count=ALL --business_type=WA_LIMITED_LIABILITY_CORPORATION",
    "wllcp": "yarn run corps company_info_scraper --concurency=1 --count=ALL --business_type=WA_LIMITED_LIABILITY_CORPORATION_PARTNERSHIP",
    "wllp": "yarn run corps company_info_scraper --concurency=1 --count=ALL --business_type=WA_LIMITED_LIABILITY_PARTNERSHIP",
    "wlp": "yarn run corps company_info_scraper --concurency=1 --count=ALL --business_type=WA_LIMITED_PARTNERSHIP",
    "wpllc": "yarn run corps company_info_scraper --concurency=1 --count=ALL --business_type=WA_PROFESSIONAL_LIMITED_LIABILITY_COMPANY",
    "wpllp": "yarn run corps company_info_scraper --concurency=1 --count=ALL --business_type=WA_PROFESSIONAL_LIMITED_LIABILITY_PARTNERSHIP",
    "wpsc": "yarn run corps company_info_scraper --concurency=1 --count=ALL --business_type=WA_PROFESSIONAL_SERVICE_CORPORATION",
    "wpc": "yarn run corps company_info_scraper --concurency=1 --count=ALL --business_type=WA_PROFIT_CORPORATION",
    "wnc": "yarn run corps company_info_scraper --concurency=1 --count=ALL --business_type=WA_NONPROFIT_CORPORATION",
    "wpbc": "yarn run corps company_info_scraper --concurency=1 --count=ALL --business_type=WA_PUBLIC_BENEFIT_CORPORATION",
    "all_keywords": "yarn run specialized && yarn run illustrations && yarn run ecocarcafe && yarn run lawfirm ",
    "all": "yarn run all_keywords && yarn run all_business",
    "refresh": "rm -rf ./node_modules ./yarn.lock && yarn install"
  },
  "bin": {
    "corps": "node dist/cli.js"
  },
  "author": "Mark Phillips",
  "license": "ISC",
  "dependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/cli-color": "^2.0.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.20",
    "@types/node-fetch": "^2.5.7",
    "@types/p-limit": "2.0.0",
    "@types/pdf-parse": "^1.1.0",
    "@types/stopword": "^0.3.0",
    "@types/yargs": "^15.0.5",
    "address-validator": "^0.2.3",
    "app-root-path": "^3.0.0",
    "axios": "^0.19.2",
    "cli-color": "^2.0.0",
    "csv": "^5.3.2",
    "delay": "^4.3.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "forever": "^3.0.0",
    "https": "^1.0.0",
    "moment": "^2.27.0",
    "mongoose": "^5.9.20",
    "morgan": "^1.10.0",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.4",
    "p-limit": "^3.0.1",
    "p-queue": "^6.5.0",
    "pdf-parse": "^1.1.1",
    "querystring": "^0.2.0",
    "socks": "^2.3.3",
    "socks-proxy-agent": "^5.0.0",
    "stopword": "^1.0.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6",
    "url": "^0.11.0",
    "winston": "^3.3.3",
    "yargs": "^15.4.0",
    "yarn-link": "^1.0.2"
  },
  "devDependencies": {
    "prettier": "2.0.5"
  }
}
